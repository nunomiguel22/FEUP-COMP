import static io.println(int) void;

class BubbleSort {
    int[] arr;
    
    public int bubbleSort(int[] arr) { 
        int n;
        int i;
        boolean sorted;
        int temp;
         
        i = 0;
        sorted = false;
        n = this.getArr().length;
        
        while(!sorted){
            sorted = true;
            i = 0;
            while (i < n - 1){
                if (arr[i + 1] < arr[i]){
                    temp = arr[i];
                    arr[i] = arr[i + 1];
                    arr[i + 1] = temp;
                    sorted = false;
                }
                else {}
                i = i + 1;
            }
        }
        return 0;
    } 
  
    public int printArray(int[] arr) { 
        int n;
        int i;
        
        {
            {
        i = 0;
        n = arr.length;
            }
        }

